Compi = require './compi'

function Leelaz extends Compi

    @: (@game) ->
        
        super @game, 'leelaz' '/opt/homebrew/bin/leelaz' [
            '-g'
            '--noponder' 
            '--timemanage' 'fast'
            ]
                                    
module.exports = Leelaz
