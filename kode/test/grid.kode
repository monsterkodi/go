    ▸ at
    
        g = new Grid 5
        
        g.fromString 
            """
            ┌───────────┐
            │ ●       ● │
            │           │
            │     ○     │
            │           │
            │ ●       ● │
            └───────────┘
            """
            
        g.at 'A1'  ▸ '●'
        g.at 0 0   ▸ '●'
        g.at 2 2   ▸ '○'
        g.at [4 4] ▸ '●'
      
    ▸ set
    
