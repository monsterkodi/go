    ▸ fromString
    
        g = new Grid 5
        
        g.fromString """
              A B C D E
            5 ●         5
            4           4
            3           3
            2           2
            1         ○ 1
              A B C D E
            """
            
        g.toString() ▸
            """
            ┌───────────┐
            │ ●         │
            │           │
            │           │
            │           │
            │         ○ │
            └───────────┘
            """
            
        g.fromString [
            '   A B C D E'
            ' 5 ●       ● 5'
            ' 4           4'
            ' 3           3'
            ' 2           2'
            ' 1 ●       ○ 1'
            '   A B C D E' ].join '\n'
            
        g.toString() ▸
            """
            ┌───────────┐
            │ ●       ● │
            │           │
            │           │
            │           │
            │ ●       ○ │
