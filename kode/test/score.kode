
Score = require '../score'

scr = (b) ->
    s = new Score b
    r = s.calcScore()
    r

ars = (b) ->
    s = new Score b
    r = s.calcScore()
    s.areaString()

ded = (b) ->
    s = new Score b
    r = s.calcScore()
