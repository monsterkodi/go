
Score = require '../score'

verbose = 1

scr = (b) ->
    s = new Score b
    r = s.calcScore verbose
    r
    
ars = (b) ->
    s = new Score b
    r = s.calcScore verbose
    s.areaString()

ded = (b) ->
    s = new Score b
    r = s.calcScore verbose
    s.deadString()

module.exports = { scr, ded, ars }

global.test = true
    
{ sgfToAlphaNum } = require '../util/util'

▸ util
    
    ▸ sgfToAlphaNum
    
        sgfToAlphaNum 'pd' ▸ 'Q16'
        sgfToAlphaNum 'dp' ▸ 'D4'
        sgfToAlphaNum 'ss' ▸ 'T1'
