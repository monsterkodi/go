
Score = require '../score'
Estimate = require '../estimate'

global.test = false
verbose = global.test

est = (b) -> 
    e = new Estimate b
    r = e.estimate verbose
    r

scr = (b) ->
    s = new Score b
    r = s.calcScore verbose
    r
    
ars = (b) ->
    s = new Score b
    r = s.calcScore verbose
    s.areaString()

ded = (b) ->
    s = new Score b
    r = s.calcScore verbose
    s.deadString()

module.exports = { scr, ded, ars, est }

{ sgfToAlphaNum } = require '../util/util'

▸ util
    
    ▸ sgfToAlphaNum
    
        sgfToAlphaNum 'pd' ▸ 'Q16'
        sgfToAlphaNum 'dp' ▸ 'D4'
        sgfToAlphaNum 'ss' ▸ 'T1'
